<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="left_arm_links">
<!--  LEFT ARM SHOULDER  -->
    <link name='left_arm_shoulder_link'>
      <origin xyz="0 0 0" rpy="0 -0 0"/>
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>

      <visual name='left_arm_shoulder_visual'>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="package://hros1_description/meshes/HROS1_Simple - Left_Arm_Shoulder.stl"/>
        </geometry>
      </visual>
      
      <inertial>
        <origin xyz="0.068 -0.052 0.131" rpy="0 -0 0"/>
        <mass value="0.011"/>
        <inertia
          ixx="0.0000051"
          ixy="0.0"
          ixz="0.00000058"
          
          iyy="0.0000023"
          iyz="0.0"
          
          izz="0.000005"
          />
      </inertial>
    </link> 



<!--  LEFT ARM UPPER SERVO  -->
    <link name='left_arm_upper_servo_link'>
      <origin xyz="0 0 0 0 -0 0"/>
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>

      <visual name='left_arm_upper_servo_visual'>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="package://hros1_description/meshes/HROS1_Simple - Left_Arm_Upper_Servo.stl"/>
        </geometry>
      </visual>
      
      <inertial>
        <origin xyz="0.085 -0.051 0.106" rpy="0 -0 0"/>
        <mass value="0.055"/>
        <inertia
          ixx="0.000015"
          ixy="0.000000062"
          ixz="0.0000017"
          
          iyy="0.000013"
          iyz="-0.00000025"
          
          izz="0.0000089"
          />
      </inertial>
    </link>


