<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="left_arm_links">
<!--  LEFT ARM SHOULDER  -->
    <link name='HR-OS1::left_arm_shoulder_link'>
      <pose>0 0 0 0 -0 0</pose>
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>

      <visual name='left_arm_shoulder_visual'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <uri>model://HR-OS1/meshes/HROS1_Simple - Left_Arm_Shoulder.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
      
      <inertial>
        <pose>0.068 -0.052 0.131 0 -0 0</pose>
        <mass>0.011</mass>
        <inertia>
          <ixx>0.0000051</ixx>
          <ixy>0.0</ixy>
          <ixz>0.00000058</ixz>
          
          <iyy>0.0000023</iyy>
          <iyz>0.0</iyz>
          
          <izz>0.000005</izz>
          </inertia>
      </inertial>
    </link> 



<!--  LEFT ARM UPPER SERVO  -->
    <link name='HR-OS1::left_arm_upper_servo_link'>
      <pose>0 0 0 0 -0 0</pose>
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>

      <visual name='left_arm_upper_servo_visual'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <uri>model://HR-OS1/meshes/HROS1_Simple - Left_Arm_Upper_Servo.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
      
      <inertial>
        <pose>0.085 -0.051 0.106 0 -0 0</pose>
        <mass>0.055</mass>
        <inertia>
          <ixx>0.000015</ixx>
          <ixy>0.000000062</ixy>
          <ixz>0.0000017</ixz>
          
          <iyy>0.000013</iyy>
          <iyz>-0.00000025</iyz>
          
          <izz>0.0000089</izz>
          </inertia>
      </inertial>
    </link>


<!--  LEFT ARM Elbow  -->
    <link name='HR-OS1::left_arm_elbow_link'>
      <pose>0 0 0 0 -0 0</pose>
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>

      <visual name='left_arm_elbow_visual'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <uri>model://HR-OS1/meshes/HROS1_Simple - Left_Arm_Elbow_Link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
      
      <inertial>
        <pose>0.093 -0.050 0.072 0 -0 0</pose>
        <mass>0.016</mass>
        <inertia>
          <ixx>0.0000036</ixx>
          <ixy>0.00000016</ixy>
          <ixz>-0.00000062</ixz>
          
          <iyy>0.0000056</iyy>
          <iyz>-0.00000064</iyz>
          
          <izz>0.0000058</izz>
          </inertia>
      </inertial>
    </link>


<!--  LEFT ARM FOREARM SERVO  -->
    <link name='HR-OS1::left_arm_forearm_servo_link'>
      <pose>0 0 0 0 -0 0</pose>
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>

      <visual name='left_arm_forearm_servo_visual'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <uri>model://HR-OS1/meshes/HROS1_Simple - Left_Arm_Forearm_Servo.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
      
      <inertial>
        <pose>0.100 -0.028 0.047 0 -0 0</pose>
        <mass>0.055</mass>
        <inertia>
          <ixx>0.000013</ixx>
          <ixy>0.0000011</ixy>
          <ixz>0.00000051</ixz>
          
          <iyy>0.000012</iyy>
          <iyz>-0.0000034</iyz>
          
          <izz>0.000012</izz>
          </inertia>
      </inertial>
    </link>
</xacro:macro>

<xacro:macro name="left_arm_joints">
  <joint name="left_shoulder_pitch_powered_joint" type="revolute">
    <parent>HR-OS1::body_structure_link</parent>
    <child>HR-OS1::left_arm_shoulder_link</child>
    <axis>
      <xyz>-1 0 0</xyz>
    </axis>
    <pose>0 -0.05 0.13 0 0 0</pose>
  </joint>
  
  
  <joint name="left_shoulder_roll_powered_joint" type="revolute">
    <parent>HR-OS1::left_arm_shoulder_link</parent>
    <child>HR-OS1::left_arm_upper_servo_link</child>
    <axis>
      <xyz>0 -1 0</xyz>
    </axis>
    <pose>-0.085 -0.05 0.115 0 0 0</pose>
  </joint>
  
  
  <joint name="left_shoulder-elbow_fixed_joint" type="revolute">
    <parent>HR-OS1::left_arm_upper_servo_link</parent>
    <child>HR-OS1::left_arm_elbow_link</child>
    <axis>
      <xyz>0 1 0</xyz>
      <limit>
          <lower>0</lower>
          <upper>0</upper>
      </limit>
    </axis>
  </joint>
  
  <joint name="left_elbow_powered_joint" type="revolute">
    <parent>HR-OS1::left_arm_elbow_link</parent>
    <child>HR-OS1::left_arm_forearm_servo_link</child>
    <axis>
      <xyz>1 0 -0.25</xyz>
    </axis>
    <pose>0 -0.037 0.03 0 0 0</pose>
  </joint>
</xacro:macro>
</robot>