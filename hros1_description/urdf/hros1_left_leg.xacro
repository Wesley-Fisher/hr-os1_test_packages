<xacro:macro name="left_leg_joints">
  <joint name="left_hip_yaw_powered_joint" type="revolute">
    <parent>HR-OS1::body_structure_link</parent>
    <child>HR-OS1::left_leg_upper_hip_link</child>
    <axis>
      <xyz>0 0 -1</xyz>
    </axis>
    <pose>-0.040 -0.045 0 0 0 0</pose>
  </joint>
  
  
  <joint name="left_hip_roll_powered_joint" type="revolute">
    <parent>HR-OS1::left_leg_upper_hip_link</parent>
    <child>HR-OS1::left_leg_lower_hip_link</child>
    <axis>
      <xyz>0 1 0</xyz>
    </axis>
    <pose>-0.040 -0.045 -0.01 0 0 0</pose>
  </joint>
  
  
  <joint name="left_hip_pitch_powered_joint" type="revolute">
    <parent>HR-OS1::left_leg_lower_hip_link</parent>
    <child>HR-OS1::left_leg_thigh_link</child>
    <axis>
      <xyz>1 0 0</xyz>
    </axis>
    <pose>-0.040 -0.045 -0.01 0 0 0</pose>
  </joint>
  
  
  <joint name="left_knee_pitch_powered_joint" type="revolute">
    <parent>HR-OS1::left_leg_thigh_link</parent>
    <child>HR-OS1::left_leg_knee_link</child>
    <axis>
      <xyz>-1 0 0</xyz>
    </axis>
    <pose>-0.015 -0.04 -0.09 0 0 0</pose>
  </joint>
  
  
  <joint name="left_knee-shin_fixed_joint" type="revolute">
    <parent>HR-OS1::left_leg_knee_link</parent>
    <child>HR-OS1::left_leg_shin_link</child>
    <axis>
      <xyz>1 0 0</xyz>
      <limit>
          <lower>0</lower>
          <upper>0</upper>
      </limit>
    </axis>
    <pose>-0.015 -0.04 -0.09 0 0 0</pose>
  </joint>
  
  
  <joint name="left_ankle_pitch_powered_joint" type="revolute">
    <parent>HR-OS1::left_leg_shin_link</parent>
    <child>HR-OS1::left_leg_ankle_link</child>
    <axis>
      <xyz>-1 0 0</xyz>
    </axis>
    <pose>-0.015 -0.045 -0.16 0 0 0</pose>
  </joint>
  
  
  <joint name="left_ankle_roll_powered_joint" type="revolute">
    <parent>HR-OS1::left_leg_ankle_link</parent>
    <child>HR-OS1::left_leg_foot_link</child>
    <axis>
      <xyz>0 -1 0</xyz>
    </axis>
    <pose>-0.04 -0.045 -0.16 0 0 0</pose>
  </joint>
</xacro:macro>
</robot>