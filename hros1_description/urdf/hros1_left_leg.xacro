<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="left_leg_links">
<!-- LEFT Upper HIP  -->
    <link name='HR-OS1::left_leg_upper_hip_link'>
      <pose>0 0 0 0 -0 0</pose>
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>

      <visual name='left_leg_upper_hip_visual'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <uri>model://HR-OS1/meshes/HROS1_Simple - Left_Leg_Yaw_Link.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
      
      <inertial>
        <pose>0.039 -0.062 0.011 0 -0 0</pose>
        <mass>0.017</mass>
        <inertia>
          <ixx>0.000016</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          
          <iyy>0.0000035</iyy>
          <iyz>0.0</iyz>
          
          <izz>0.000015</izz>
          </inertia>
      </inertial>
    </link>  

