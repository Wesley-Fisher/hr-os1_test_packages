<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
test
<xacro:macro name="neck_links">
    <!--  ATTACHED TO TOP OF BODY -->
    <link name='HR-OS1::neck_link_1_link'>
      <origin xyz="0 0 0" rpy="0 -0 0"/>
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>

      <visual name='neck_link_1_visual'>
        <origin xyz="0 0 0 0 -0 0"/>
        <geometry>
          <mesh filename="package://hros1_description/meshes/HROS1_Simple - Neck_Link_1.stl"/>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material"
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
      
      <inertial>
        <origin xyz="0 -0.101 0.208 0 -0 0"/>
        <mass>0.005</mass>
        <inertia>
          <ixx>0.0000012</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          
          <iyy>0.0000032</iyy>
          <iyz>0.0</iyz>
          
          <izz>0.000002</izz>
          </inertia>
      </inertial>
    </link>
</xacro:macro>


<xacro:macro name="neck_joints">
  <joint name="neck_yaw_powered_joint" type="revolute">
    <parent>HR-OS1::body_structure_link</parent>
    <child>HR-OS1::neck_base_link</child>
    <axis>
      <xyz>0 0 1</xyz>
    </axis>
    <origin xyz="0 -0.045 0" rpy="0 0 0"/>
  </joint>


  <joint name="neck_base_platform_free_joint" type="revolute">
    <parent>HR-OS1::neck_base_link</parent>
    <child>HR-OS1::neck_base_platform_link</child>
    <axis>
      <xyz>1 0 0</xyz>
    </axis>
    <origin xyz="0 -0.046 0.2" rpy="0 0 0"/>
  </joint>
  

  <joint name="neck_pitch_1_free_joint" type="revolute">
    <parent>HR-OS1::neck_base_link</parent>
    <child>HR-OS1::neck_link_1_link</child>
    <axis>
      <xyz>1 0 0</xyz>
    </axis>
    <origin xyz="0 -0.105 0.19" rpy="0 0 0"/>
  </joint>
  
  <joint name="neck_pitch_2_free_joint" type="revolute">
    <parent>HR-OS1::neck_link_1_link</parent>
    <child>HR-OS1::neck_link_2_link</child>
    <axis>
      <xyz>1 0 0</xyz>
    </axis>
    <origin xyz="0 -0.105 0.23" rpy="0 0 0"/>
  </joint>
  
  
  <joint name="neck_pitch_main_powered_joint" type="revolute">
    <parent>HR-OS1::neck_link_2_link</parent>
    <child>HR-OS1::neck_head_link</child>
    <axis>
      <xyz>-1 0 0</xyz>
    </axis>
    <origin xyz="0 -0.07 0.23" rpy="0 0 0"/>
  </joint>
</xacro:macro>
</robot>
