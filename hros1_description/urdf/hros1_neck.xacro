<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
test
<xacro:macro name="neck_links">
    <!--  ATTACHED TO TOP OF BODY -->
    <link name='HR-OS1::neck_link_1_link'>
      <origin xyz="0 0 0" rpy="0 -0 0"/>
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>

      <visual name='neck_link_1_visual'>
        <origin xyz="0 0 0" rpy="0 -0 0"/>
        <geometry>
          <mesh filename="package://hros1_description/meshes/HROS1_Simple - Neck_Link_1.stl"/>
        </geometry>
      </visual>
      
      <inertial>
        <origin xyz="0 -0.101 0.208" rpy="0 -0 0"/>
        <mass value="0.005"/>
        <inertia
          ixx="0.0000012"
          ixy="0.0"
          ixz="0.0"
          
          iyy="0.0000032"
          iyz="0.0"
          
          izz="0.000002"
          />
      </inertial>
    </link>
</xacro:macro>


<xacro:macro name="neck_joints">
  <joint name="neck_yaw_powered_joint" type="revolute">
    <parent link="HR-OS1::body_structure_link"/>
    <child link="HR-OS1::neck_base_link"/>
    <limit effort="1" velocity="1" lower="-3.1415" upper="3.1415"/>
    <axis>
      <xyz>0 0 1</xyz>
    </axis>
    <origin xyz="0 -0.045 0" rpy="0 0 0"/>
  </joint>


  <joint name="neck_base_platform_free_joint" type="revolute">
    <parent link="HR-OS1::neck_base_link"/>
    <child link="HR-OS1::neck_base_platform_link"/>
    <limit effort="1" velocity="1" upper="3.1415" lower="-3.1415"/>
    <axis>
      <xyz>1 0 0</xyz>
    </axis>
    <origin xyz="0 -0.046 0.2" rpy="0 0 0"/>
  </joint>
  

  <joint name="neck_pitch_1_free_joint" type="revolute">
    <parent link="HR-OS1::neck_base_link"/>
    <child link="HR-OS1::neck_link_1_link"/>
    <limit effort="1" velocity="1" upper="3.1415" lower="-3.1415"/>
    <axis>
      <xyz>1 0 0</xyz>
    </axis>
    <origin xyz="0 -0.105 0.19" rpy="0 0 0"/>
  </joint>
  
  <joint name="neck_pitch_2_free_joint" type="revolute">
    <parent link="HR-OS1::neck_link_1_link"/>
    <child link="HR-OS1::neck_link_2_link"/>
    <limit effort="1" velocity="1" upper="3.1415" lower="-3.1415"/>
    <axis>
      <xyz>1 0 0</xyz>
    </axis>
    <origin xyz="0 -0.105 0.23" rpy="0 0 0"/>
  </joint>
  
  
  <joint name="neck_pitch_main_powered_joint" type="revolute">
    <parent link="HR-OS1::neck_link_2_link"/>
    <child link="HR-OS1::neck_head_link"/>
    <limit effort="1" velocity="1" upper="3.1415" lower="-3.1415"/>
    <axis>
      <xyz>-1 0 0</xyz>
    </axis>
    <origin xyz="0 -0.07 0.23" rpy="0 0 0"/>
  </joint>
</xacro:macro>
</robot>
